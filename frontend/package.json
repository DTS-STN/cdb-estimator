{
  "$schema": "https://json.schemastore.org/package",
  "name": "cdb-estimator-frontend",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "npm run build:application && npm run build:server",
    "build:application": "react-router build",
    "build:server": "vite build --config ./vite.server.config.ts",
    "dev": "cross-env DEV_ENDPOINTS_ENABLED=true nodemon --config nodemon.json",
    "preview": "npm run build && cross-env NODE_ENV=production SESSION_COOKIE_SECURE=false node --env-file-if-exists=.env --import ./build/server/telemetry.js ./build/server/server.js",
    "start": "cross-env NODE_ENV=production node --import ./build/server/telemetry.js ./build/server/server.js",
    "checks": "node --experimental-strip-types ./scripts/checks-runner.ts",
    "test": "cross-env DEV_ENDPOINTS_ENABLED=true vitest ",
    "test:e2e": "cross-env DEV_ENDPOINTS_ENABLED=true playwright test",
    "format:check": "prettier --cache --check .",
    "format:write": "prettier --write .",
    "lint:check": "eslint --cache ./",
    "lint:summary": "eslint --format summary .",
    "typecheck": "react-router typegen && tsc"
  },
  "dependencies": {
    "@axe-core/playwright": "^4.10.1",
    "@date-fns/tz": "^1.2.0",
    "@dts-stn/health-checks": "^2.0.0",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.62.2",
    "@opentelemetry/exporter-metrics-otlp-proto": "^0.204.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.204.0",
    "@opentelemetry/resources": "^2.1.0",
    "@opentelemetry/sdk-metrics": "^2.1.0",
    "@opentelemetry/sdk-node": "^0.204.0",
    "@opentelemetry/sdk-trace-node": "^2.1.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "@radix-ui/react-dialog": "^1.1.14",
    "@react-router/express": "^7.9.4",
    "@react-router/node": "^7.9.4",
    "@react-router/serve": "^7.9.4",
    "clsx": "^2.1.1",
    "compression": "^1.8.1",
    "connect-redis": "^8.1.0",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "express": "^5.2.1",
    "express-session": "^1.18.2",
    "i18next": "^25.2.1",
    "i18next-fetch-backend": "^6.0.0",
    "ioredis": "^5.6.1",
    "isbot": "^5.1.28",
    "minimatch": "^10.0.1",
    "morgan": "^1.10.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.5.2",
    "react-idle-timer": "^5.7.2",
    "react-number-format": "^5.4.4",
    "react-router": "^7.9.4",
    "source-map-support": "^0.5.21",
    "tailwind-merge": "^3.3.0",
    "valibot": "^1.2.0",
    "winston": "^3.17.0",
    "winston-error-format": "^3.0.2"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.9",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.27.0",
    "@inquirer/checkbox": "^4.1.6",
    "@playwright/test": "^1.57.0",
    "@react-router/dev": "^7.9.4",
    "@tailwindcss/vite": "^4.1.7",
    "@testing-library/react": "^16.3.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/compression": "^1.8.0",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/express-session": "^1.18.1",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@types/source-map-support": "^0.5.10",
    "@types/validator": "^13.15.1",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^3.1.4",
    "eslint": "^9.27.0",
    "eslint-formatter-summary": "^2.0.2",
    "eslint-import-resolver-typescript": "^4.3.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unicorn": "^59.0.1",
    "globals": "^16.1.0",
    "jsdom": "^26.1.0",
    "nodemon": "^3.1.10",
    "ora": "^8.2.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.1.6",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.4.1",
    "vite-plugin-static-copy": "^3.1.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.3",
    "vitest-mock-extended": "^3.1.0"
  },
  "engines": {
    "node": ">=22"
  },
  "overrides": {
    "vite": {
      "rollup": "npm:@rollup/wasm-node@^4.34.8"
    }
  }
}
